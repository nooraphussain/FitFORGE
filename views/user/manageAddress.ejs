                                                              <script>
                                                              function toggleEditForm(addrId) {
                                                                var form = document.getElementById("edit-form-" + addrId);
                                                                if (form.style.display === "none" || form.style.display === "") {
                                                                  form.style.display = "block";
                                                                } else {
                                                                  form.style.display = "none";
                                                                }
                                                              }
                                                              
                                                              function deleteAddress(addrId) {
                                                                Swal.fire({
                                                                    title: 'Are you sure?',
                                                                    text: "Do you really want to delete this address?",
                                                                    icon: 'warning',
                                                                    showCancelButton: true,
                                                                    confirmButtonColor: '#3085d6',
                                                                    cancelButtonColor: '#d33',
                                                                    confirmButtonText: 'Yes, delete it!',
                                                                    cancelButtonText: 'Cancel'
                                                                }).then((result) => {
                                                                    if (result.isConfirmed) {
                                                                    // Create a temporary form and submit it to delete the address
                                                                    var form = document.createElement("form");
                                                                    form.method = "POST";
                                                                    form.action = "/account/addresses";
                                                                    
                                                                    var actionInput = document.createElement("input");
                                                                    actionInput.type = "hidden";
                                                                    actionInput.name = "action";
                                                                    actionInput.value = "delete";
                                                                    form.appendChild(actionInput);
                                                                    
                                                                    var idInput = document.createElement("input");
                                                                    idInput.type = "hidden";
                                                                    idInput.name = "addressId";
                                                                    idInput.value = addrId;
                                                                    form.appendChild(idInput);
                                                                    
                                                                    document.body.appendChild(form);
                                                                    form.submit();
                                                                    }
                                                                });
                                                                }

                                                              
                                                              // Dropdown toggle for each address block
                                                              document.addEventListener("DOMContentLoaded", function() {
                                                                var dropdowns = document.querySelectorAll("[id^='dropdownMenu-']");
                                                                dropdowns.forEach(function(dropdown) {
                                                                  dropdown.addEventListener("mouseenter", function() {
                                                                    var options = this.querySelector(".dropdown-options");
                                                                    if (options) options.style.display = "block";
                                                                  });
                                                                  dropdown.addEventListener("mouseleave", function() {
                                                                    var options = this.querySelector(".dropdown-options");
                                                                    if (options) options.style.display = "none";
                                                                  });
                                                                });
                                                              });
                                                              </script>                                                              

