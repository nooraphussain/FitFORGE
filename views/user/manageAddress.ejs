      dropdownContainer.addEventListener("mouseenter", function() {
        dropdown.style.display = "block";
      });
      dropdownContainer.addEventListener("mouseleave", function() {
        dropdown.style.display = "none";
      });
    });
</script>

<script>
    function updateRadioStyles() {
      var homeRadio = document.getElementById('radioHome');
      var workRadio = document.getElementById('radioWork');
      var customHome = document.getElementById('customRadioHome');
      var customWork = document.getElementById('customRadioWork');
      
      // Update custom radio indicators based on selection
      if(homeRadio.checked) {
        customHome.style.backgroundColor = '#000000';
      } else {
        customHome.style.backgroundColor = 'transparent';
      }
      
      if(workRadio.checked) {
        customWork.style.backgroundColor = '#000000';
      } else {
        customWork.style.backgroundColor = 'transparent';
      }
    }
</script>
<script>
document.addEventListener("DOMContentLoaded", function () {
    const addAddressBtn = document.querySelector(".add-address-btn");
    const addressFormContainer = document.querySelector(".address-form-container");
    const cancelButton = document.querySelector(".cancel-btn");

    // Initially hide the address form
    addressFormContainer.style.display = "none";

    addAddressBtn.addEventListener("click", function () {
        addressFormContainer.style.display = "block";
        addAddressBtn.style.display = "none"; // Hide the button
    });

    cancelButton.addEventListener("click", function () {
        addressFormContainer.style.display = "none";
        addAddressBtn.style.display = "flex"; // Show the button back
    });
});



</script>

    
    
